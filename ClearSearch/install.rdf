<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>clearsearch@example.net</em:id>
    <em:version>1.0</em:version>
    <em:type>2</em:type>
    <em:iconURL>chrome://ClearSearch/skin/icon.png</em:iconURL>
   
    <!-- Target Application this extension can install into, 
         with minimum and maximum supported versions. --> 
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>4.0.*</em:minVersion>
        <em:maxVersion>25.0</em:maxVersion>
      </Description>
    </em:targetApplication>
   
    <!-- Front End MetaData -->
    <em:name>ClearSearch</em:name>
    <em:description>Adds option in Privacy settings to auto-clear Searchbox</em:description>
    <em:creator>Arul Jeyaseelan and Ankit Khandelwal</em:creator>
    <em:homepageURL>http://www.example.com/</em:homepageURL>
  </Description>      
</RDF>
